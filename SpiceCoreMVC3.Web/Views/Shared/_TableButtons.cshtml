@model string

<div class="" role="group">
    <a class="btn btn-success text-white" asp-action="Edit" asp-route-id="@Model">
        <i class="fas fa-edit"></i>
    </a>
    <a class="btn btn-info text-white" asp-action="Details" asp-route-id="@Model">
        <i class="fas fa-list"></i>
    </a>
    <a class="btn btn-danger text-white" asp-action="Delete" asp-route-id="@Model">
        <i class="fas fa-trash"></i>
    </a>
</div>
