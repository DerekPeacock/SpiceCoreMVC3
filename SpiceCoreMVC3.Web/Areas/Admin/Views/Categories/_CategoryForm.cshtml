@model Category


<div asp-validation-summary="ModelOnly" class="text-danger"></div>
<div class="form-group">

    <label asp-for="Id" class="control-label"></label>
    <input asp-for="Id" type="text" class="form-control" placeholder="Enter Category Name" />
    <span asp-validation-for="Id" class="text-danger"></span>

    <label asp-for="MenuOrder" class="control-label"></label>
    <input asp-for="MenuOrder" type="number" class="form-control" />
    <span asp-validation-for="MenuOrder" class="text-danger"></span>

</div>

<partial name="_FormButtons" />
